# Local/private config goes in the include
[include]
  path = ~/.gitconfig.local
[color]
  ui = auto
[color "branch"]
  current = cyan bold reverse
  local = white
  plain =
  remote = red
[color "diff"]
  commit =
  func =
  plain =
  whitespace = red reverse
  meta = white
  frag = cyan bold reverse
  old = red
  new = green
[color "grep"]
[color "interactive"]
[color "status"]
  added = green bold
  changed = white
  header =
  localBranch =
  nobranch =
  remoteBranch =
